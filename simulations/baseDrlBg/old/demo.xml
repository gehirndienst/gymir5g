<config>
    <!-- Assign to the cellular interface of all e/gNodeBs and UEs one address of type 10.x.x.x -->
    <interface hosts="eNodeB*" names="cellular" address="10.x.x.x" netmask="255.0.0.0"/>
    <interface hosts="gNodeB*" names="cellular" address="10.x.x.x" netmask="255.0.0.0"/>
    <interface hosts="ship*" names="cellular" address="10.x.x.x" netmask="255.0.0.0"/>
    
    <!-- Assign 192.168.x.x addresses to all other interfaces -->
    <interface hosts="*" address="192.168.x.x" netmask="255.255.255.0"/>
</config>