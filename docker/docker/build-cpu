TAG="latest"

while (( $# >= 1 )); do
    case "$1" in
    -t | --tag)
            TAG="$2"
            shift 2
            ;;
    -*)
            echo "An unknown option: $1" >&2
            exit 1 
            ;;
    --)
            shift
            break
            ;;
    *)
            break
            ;;
    esac
done

docker build --build-arg ACCESS_TOKEN=$(cat access_token.txt) -t gymir5g-cpu -t gymir5g-cpu:$TAG -f Dockerfile-cpu .