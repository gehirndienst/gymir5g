  "C_Cpp.workspaceParsingPriority": "medium",
  "C_Cpp.intelliSenseCacheSize": 0,
  "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.deps": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "/bin": true,
    "/boot": true,
    "/cdrom": true,
    "/dev": true,
    "/proc": true,
    "/etc": true
  },
  "editor.defaultFormatter": null,
  "editor.formatOnSave": true,
  "[json]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.blackArgs": [
    "--line-length",
    "120",
    "--skip-string-normalization",
    "--preview"
  ],
  "python.formatting.provider": "none",
  "python.analysis.typeCheckingMode": "off",
  "[c]": {
    "editor.defaultFormatter": "chiehyu.vscode-astyle"
  },
  "[cpp]": {
    "editor.defaultFormatter": "chiehyu.vscode-astyle"
  },
  "astyle.additional_languages": [
    "c",
    "cpp"
  ],
  "astyle.java.enable": true,
  "astyle.cmd_options": [
    "--style=java",
    "--align-pointer=type",
    "--align-reference=type",
    "--attach-closing-while",
    //"--indent-after-parens",
    "--indent-continuation=1",
    "--indent-col1-comments",
    "--indent-namespaces",
    "--indent-switches",
    "--keep-one-line-blocks",
    "--keep-one-line-statements",
    "--pad-oper",
    "--max-code-length=120",
    "--lineend=linux"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "/opt/omnetpp-workspace/gymir5g/drl/gymirdrl/configurations/experiments/*/**": true,
    "/opt/omnetpp-workspace/gymir5g/drl/gymirdrl/logs/*/**": true,
    "/opt/omnetpp-workspace/gymir5g/drl/gymirdrl/models/*/**": true
  },
  "todo-tree.tree.autoRefresh": true,
  "todo-tree.highlights.customHighlight": {
    "WARN": {
      "foreground": "#000000",
      "background": "#FFA500",
      "iconColour": "#FFA500",
      "icon": "eye",
    },
    "TODO": {
      "foreground": "#000000",
      "background": "#FFD700",
      "iconColour": "#FFD700",
      "icon": "check",
    },
    "FIXME": {
      "foreground": "#000000",
      "background": "#FF69B4",
      "iconColour": "#FF69B4",
      "icon": "bug"
    },
    "HACK": {
      "foreground": "#000000",
      "background": "#008000",
      "iconColour": "#008000",
      "icon": "alert"
    }
  },
  "todo-tree.highlights.defaultHighlight": {
    "type": "tag"
  },
  "todo-tree.general.tags": [
    "TODO",
    "FIXME",
    "HACK",
    "WARN"
  ]
}